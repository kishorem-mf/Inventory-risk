# Inventory Risk Application - Environment Variables
# Copy this file to .env and fill in your actual values

# SAP AI Core Configuration
AICORE_AUTH_URL=https://your-tenant.authentication.region.hana.ondemand.com
AICORE_CLIENT_ID=your-client-id
AICORE_CLIENT_SECRET=your-client-secret
AICORE_BASE_URL=https://api.ai.prod.region.aws.ml.hana.ondemand.com
AICORE_RESOURCE_GROUP=default

# SAP HANA Database
HANA_HOST=your-hana-host.hanacloud.ondemand.com
HANA_USER=DBADMIN
HANA_PASSWORD=your-password

# LangSmith (optional - for tracing)
LANGSMITH_API_KEY=your-langsmith-api-key

# Tavily Search (optional)
TAVILY_API_KEY=your-tavily-api-key

# Azure OpenAI (for knowledge graph notebooks)
AZURE_OPENAI_API_KEY=your-azure-openai-key
AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com/

# Secondary Azure OpenAI instance (optional)
AZURE_OPENAI_API_KEY_2=your-secondary-key
AZURE_OPENAI_ENDPOINT_2=https://your-secondary-resource.openai.azure.com/

# Neo4j (for knowledge graph)
NEO4J_URI=neo4j+ssc://your-instance.databases.neo4j.io
NEO4J_USERNAME=neo4j
NEO4J_PASSWORD=your-neo4j-password
NEO4J_DATABASE=neo4j
